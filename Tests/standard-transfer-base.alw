begin
   real r;

   procedure w (real value x);
   begin
      write( base10(x), " ", longbase10(x));
   end;

   w(0);
   w(1);
   w(10);
   w(16);
   w(32);
   w(-91'101);
   w(91'-101);
   write(" ");

   r := 1.23456789012345;
   for i := 0 until 10 do
      begin
         w(r * 10**i);
         w(r * 10**-i);
         w(-r * 10**i);
         w(-r * 10**-i)
      end
end.
